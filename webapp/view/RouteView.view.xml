<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="weasel.challenge.controller.StartView"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:l="sap.ui.layout" 
	xmlns:f="sap.ui.layout.form" >
	<f:SimpleForm 
	class="Tabelle"
	id="Tabelle">
	<Table id="Route" items="{}">
		 			<columns>
			<Column
				width="20%">
				<Text text="Typ" />
			</Column>
			<Column
				width="20%"
				demandPopin="true">
				<Text text="Nummer" />
			</Column>
			</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{Typ}"/>
									<Text text="{Nummer}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
					</f:SimpleForm>
	
</mvc:View>